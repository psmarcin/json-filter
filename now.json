{
  "name": "podcast",
  "env":{
    "PS_GOOGLE_API": "@ps_google_api",
    "PORT":"8080",
    "REMOTE_URL": "https://podcast.psmarcin.me"
  },
  "builds": [
    {
      "src": "main.go",
      "use": "@now/go"
    }
  ],
  "routes": [
    {
      "src": "/example",
      "dest": "/404.html",
      "status": 404
    }
  ],
  "version": 2,
  "scale": {
    "bru1": {
      "min": 0,
      "max": 1
    },
    "sfo1":{
      "min": 0,
      "max": 1
    }
  }
}
